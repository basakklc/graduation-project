<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/item_background"
    tools:context=".presentation.main.view.ProfilEkraniFragment">

    <ImageButton
        android:id="@+id/ic_ayarlar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="348dp"
        android:layout_marginTop="48dp"
        android:background="@drawable/ic_ayarlar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="250dp"
        android:background="@drawable/profil_radius"
        android:orientation="vertical">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            android:layout_marginLeft="15dp"
            android:layout_marginBottom="30dp"
            android:text="Başak Kılıç"
            android:textColor="@color/formItemHintText"
            android:textSize="22sp"
            android:fontFamily="@font/semibold"
            android:paddingLeft="@dimen/text_margin"
            android:paddingTop="9dp"
            android:paddingBottom="8dp"/>


        <RelativeLayout
            android:layout_width="382dp"
            android:layout_height="290dp"
            android:layout_marginStart="14dp"
            android:layout_marginEnd="14dp"
            android:background="@drawable/raw_item_radius"
            android:paddingTop="45dp"
            android:paddingBottom="45dp">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/evark"
                android:layout_marginLeft="6dp"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="45dp"
                    android:background="@drawable/ic_evark"/>

                <TextView
                    android:layout_width="105dp"
                    android:layout_height="46dp"
                    android:layout_marginLeft="15dp"

                    android:layout_marginBottom="30dp"
                    android:fontFamily="@font/medium"
                    android:paddingLeft="@dimen/text_margin"
                    android:paddingTop="9dp"
                    android:paddingBottom="8dp"
                    android:text="Ev Arkadaşı Ilanlarım"
                    android:textColor="@color/itemSubTitleText"
                    android:textSize="@dimen/text_smallsize" />

            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/esya"
                android:layout_toEndOf="@id/evark"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="45dp"
                    android:background="@drawable/ic_esya"/>

                <TextView
                    android:layout_width="105dp"
                    android:layout_height="46dp"
                    android:layout_marginLeft="15dp"

                    android:layout_marginBottom="30dp"
                    android:fontFamily="@font/medium"
                    android:paddingLeft="@dimen/text_margin"
                    android:paddingTop="9dp"
                    android:paddingBottom="8dp"
                    android:text="Eşya Ilanlarım"
                    android:textColor="@color/itemSubTitleText"
                    android:textSize="@dimen/text_smallsize" />


            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ev"
                android:layout_toEndOf="@id/esya"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="45dp"
                    android:background="@drawable/ic_evilanlarim"/>

                <TextView
                    android:layout_width="105dp"
                    android:layout_height="46dp"
                    android:layout_marginLeft="15dp"

                    android:layout_marginBottom="30dp"
                    android:fontFamily="@font/medium"
                    android:paddingLeft="@dimen/text_margin"
                    android:paddingTop="9dp"
                    android:paddingBottom="8dp"
                    android:text="Ev Ilanlarım"
                    android:textColor="@color/itemSubTitleText"
                    android:textSize="@dimen/text_smallsize" />


            </LinearLayout>

            <View
                android:layout_width="360dp"
                android:layout_height="1.5dp"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="100dp"
                android:background="@color/layout_background"/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/etkinlik"
                android:layout_marginLeft="6dp"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="120dp"
                    android:layout_marginLeft="45dp"
                    android:id="@+id/ic_etkinlikIlani"
                    android:background="@drawable/ic_etkinlikilani"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="46dp"
                    android:layout_marginLeft="5dp"

                    android:layout_marginBottom="30dp"
                    android:fontFamily="@font/medium"
                    android:paddingLeft="@dimen/text_margin"
                    android:paddingTop="9dp"
                    android:paddingBottom="8dp"
                    android:text="Etkinlik Ilanlarım"
                    android:textColor="@color/itemSubTitleText"
                    android:textSize="@dimen/text_smallsize" />

            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/ders"
                android:layout_toEndOf="@id/evark"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="45dp"
                    android:layout_marginTop="120dp"
                    android:background="@drawable/ic_dersilanlari"/>

                <TextView
                    android:layout_width="105dp"
                    android:layout_height="46dp"
                    android:layout_marginLeft="15dp"


                    android:layout_marginBottom="30dp"
                    android:fontFamily="@font/medium"
                    android:paddingLeft="@dimen/text_margin"
                    android:paddingTop="9dp"
                    android:paddingBottom="8dp"
                    android:text="Ders Ilanlarım"
                    android:textColor="@color/itemSubTitleText"
                    android:textSize="@dimen/text_smallsize" />


            </LinearLayout>




        </RelativeLayout>


    </LinearLayout>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/profil"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="55dp"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="57dp" />


</androidx.constraintlayout.widget.ConstraintLayout>